<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE Workers SYSTEM "elements.dtd">
<Workers>
    <Worker>
        <Name>Split by Age</Name>
        <ThreadCount>3</ThreadCount>
        <Inputs>
            <Input>
                <Name>SQL Reader</Name>
                <ThreadCount>2</ThreadCount>
                <Parameters>
                    <Parameter>
                        <Key>database</Key>
                        <Value>jdbc:mysql://localhost:3306/raf</Value>
                    </Parameter>
                    <Parameter>
                        <Key>username</Key>
                        <Value>root</Value>
                    </Parameter>
                    <Parameter>
                        <Key>password</Key>
                        <Value></Value>
                    </Parameter>
                </Parameters>
            </Input>
        </Inputs>
        <Outputs>
            <Output>
                <Name>Log Writer</Name>
                <ThreadCount>4</ThreadCount>
                <Parameters></Parameters>
            </Output>
        </Outputs>
        <Parameters>
            <Parameter>
                <Key>Start</Key>
                <Value>true</Value>
            </Parameter>
            <Parameter>
                <Key>age</Key>
                <Value>20-</Value>
            </Parameter>
        </Parameters>
    </Worker>
    <Worker>
        <Name>Find Average</Name>
        <ThreadCount>2</ThreadCount>
        <Inputs>
        </Inputs>
        <Outputs>
            <Output>
                <Name>Log Writer</Name>
                <ThreadCount>4</ThreadCount>
                <Parameters>
                    <Parameter>
                        <Key>Destination</Key>
                        <Value>URL</Value>
                    </Parameter>
                </Parameters>
            </Output>
            <Output>
                <Name>GUI Display</Name>
                <ThreadCount>1</ThreadCount>
                <Parameters></Parameters>
            </Output>
            <Output>
                <Name>PDF Writer</Name>
                <ThreadCount>3</ThreadCount>
                <Parameters></Parameters>
            </Output>
        </Outputs>
        <Parameters>
            <Parameter>
                <Key>End</Key>
                <Value>true</Value>
            </Parameter>
        </Parameters>
    </Worker>
</Workers>
